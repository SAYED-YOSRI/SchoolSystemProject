@model List<final_project.Models.ExamResult>

<h2>My Grades</h2>

<table class="table">
    <thead>
        <tr>
            <th>Course</th>
            <th>Exam</th>
            <th>Marks</th>
            <th>Grade</th>
            <th>Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var result in Model)
        {
            <tr>
                <td>@result.Exam.Course.Title</td>
                <td>@result.Exam.ExamName</td>
                <td>@result.MarksObtained</td>
                <td>@result.Grade</td>
                <td>@result.ResultDate.ToShortDateString()</td>
            </tr>
        }
    </tbody>
</table>
