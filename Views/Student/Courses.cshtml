@model List<final_project.Models.Enrollment>

<h2>My Courses</h2>

<table class="table">
    <thead>
        <tr>
            <th>Course Title</th>
            <th>Teacher</th>
            <th>Department</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var e in Model)
        {
            <tr>
                <td>@e.Course.Title</td>
                <td>@e.Course.Teacher?.FullName</td>
                <td>@e.Course.Department?.Name</td>
            </tr>
        }
    </tbody>
</table>

